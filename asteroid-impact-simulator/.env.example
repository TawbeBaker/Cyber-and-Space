# NASA API Configuration
NASA_API_KEY=DEMO_KEY

# USGS Configuration (no key required for public data)
USGS_ELEVATION_API=https://epqs.nationalmap.gov/v1
USGS_EARTHQUAKE_API=https://earthquake.usgs.gov/fdsnws/event/1

# Application Configuration
NODE_ENV=development
PORT=7071
FRONTEND_URL=http://localhost:3000

# Cache Configuration
CACHE_TTL=3600

# Azure Application Insights (optional for production)
APPINSIGHTS_INSTRUMENTATIONKEY=

# Physics Engine Configuration
DEFAULT_ASTEROID_DENSITY=3000  # kg/m³ (typical rocky asteroid)
EARTH_RADIUS=6371000  # meters
GRAVITATIONAL_CONSTANT=6.67430e-11  # m³/kg·s²
